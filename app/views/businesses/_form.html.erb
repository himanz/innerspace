<div class="form">
  <%= form_for @business do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :country %>
    <%= f.text_field :country %>

    <%= f.label :latitude %>
    <%= f.number_field :latitude, :step => 'any' %>

    <%= f.label :longitude %>
    <%= f.number_field :longitude, :step => 'any' %>

    <%= f.label :heading %>
    <%= f.number_field :heading, :step => 'any' %>

    <%= f.label :pano %>
    <%= f.text_field :pano %>

    <%= f.label :cbp %>
    <%= f.text_field :cbp %>

    <%= f.label :category %>
    <%= f.collection_select :category_id, Category.all, :id, :name, include_blank: true %>

    <%= f.submit :Submit, :class => "button" %>
  <% end %>
</div>