<%= form_for @business do |f| %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :heading %>
  <%= f.number_field :heading, :step => 'any' %>

  <%= f.label :pano %>
  <%= f.text_field :pano %>

  <%= f.submit :Submit %>
<% end %>