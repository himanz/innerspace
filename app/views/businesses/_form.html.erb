<%= form_for @business do |f| %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :heading %>
  <%= f.number_field :heading, :step => 'any' %>

  <%= f.label :pano %>
  <%= f.text_field :pano %>

  <%= f.label :cbp %>
  <%= f.text_field :cbp %>

  <%= f.collection_select :category_id, Category.all, :id, :name, include_blank: true %>

  <%= f.submit :Submit %>
<% end %>